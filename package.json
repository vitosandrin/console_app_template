{
  "name": "console_app_template",
  "version": "0.0.1",
  "description": "A console application template for node.js using typescript",
  "author": "Vito Sandrin <sandrinvito10@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/vitosandrin/console_app_template.git"
  },
  "keywords": [],
  "license": "ISC",
  "main": "dist/index.js",
  "scripts": {
    "test": "ts-node node_modules/jasmine/bin/jasmine --config=jasmine.json",
    "build": "tsc --build",
    "clean": "tsc --build --clean",
    "start": "node ./dist/index.js",
    "dev": "ts-node-dev --transpile-only ./src/index.ts",
    "style:check": "prettier --check ./src/**/*.ts",
    "style:fix": "prettier --write ./src/**/*.ts"
  },
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/date-fns": "^2.6.0",
    "@types/figlet": "^1.5.8",
    "@types/jasmine": "^5.1.4",
    "@types/moment-timezone": "^0.5.30",
    "@types/node": "^20.9.3",
    "@types/pino": "^7.0.5",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "jasmine": "^5.1.0",
    "prettier": "^3.1.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "date-fns": "^2.30.0",
    "figlet": "^1.7.0",
    "moment-timezone": "^0.5.43",
    "pino": "^8.16.2",
    "winston": "^3.11.0"
  }
